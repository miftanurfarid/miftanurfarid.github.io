	date = {2022-05},
	note = {{ISSN}: 2379-190X},
	keywords = {deep learning, Array signal processing, beamforming, Measurement, multi-channel speech enhancement, multi-microphone complex spectral mapping, Neural networks, Pipelines, Speech enhancement, Three-dimensional displays, Wiener filters},
	file = {IEEE Xplore Abstract Record:/home/fafa/Zotero/storage/FBC3TPMA/9747146.html:text/html;Submitted Version:/home/fafa/Zotero/storage/Z5YEFLCM/Lu et al. - 2022 - Towards Low-Distortion Multi-Channel Speech Enhancement The ESPNET-Se Submission to the L3DAS22 Cha.pdf:application/pdf},
