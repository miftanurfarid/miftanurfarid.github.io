@article{faridPEMISAHANSUARATERCAMPUR2019,
	title = {{PEMISAHAN} {SUARA} {TERCAMPUR} {BERDASARKAN} {KARAKTERISTIK} {BINAURAL}},
	volume = {7},
	rights = {Copyright (c) 2019 {JTT} (Jurnal Teknologi Terpadu)},
	issn = {2477-5177},
	url = {https://jurnal.poltekba.ac.id/index.php/jtt/article/view/734},
	doi = {10.32487/jtt.v7i2.734},
	abstract = {Dalam suatu percakapan di pesta cocktail, seseorang mampu memfokuskan pendengarannya pada seorang lawan bicaranya meskipun bunyi musik latar cukup keras dan disertai percakapan beberapa orang lainnya. Fenomena ini dikenal dengan the cocktail party effect. Dalam sebuah studi awal dijelaskan bahwa binaural hearing memberikan kontribusi penting dalam efek tersebut. Penelitian tentang pemisahan suara yang berdasarkan pada lokalisasi bunyi telah dilakukan sebelumnya. Pada penelitian tersebut, pemisahan suara tercampur hanya berdasarkan binaural cue - interaural time difference. Maka pada penelitian ini, akan dilakukan pemisahan suara dari input binaural dengan 2 sensor mikrofon dari dua sumber suara berdasarkan kedua binaural cue tersebut yaitu interaural time difference ({ITD}) dan interaural level difference ({ILD}) menggunakan binary mask. Untuk mengestimasi nilai {ITD}, digunakan metode cross-correlation yang nilai {ITD} direpresentasikan sebagai nilai time delay pada pergeseran peak pada unit time-frequency. Binary mask diestimasi berdasarkan pola perubahan nilai interaural time difference dan interaural level difference terhadap nilai relative strength dari sinyal target yang dihitung secara statistik menggunakan probability density estimation. Hasil pemisahan sumber suara tercampur menunjukkan performa yang baik dengan {SNR} sebesar 3 {dB}.},
	pages = {119--124},
	number = {2},
	journaltitle = {{JTT} (Jurnal Teknologi Terpadu)},
	author = {Farid, Mifta Nur},
	urldate = {2020-07-19},
	date = {2019-10-30},
	langid = {indonesian},
	note = {Number: 2},
	keywords = {binaural hearing, cocktail-party, {ILD}, {ITD}, percent correct word, {SNR}.},
	file = {Full Text PDF:/home/fafa/Zotero/storage/VDY9MRWR/Farid - 2019 - PEMISAHAN SUARA TERCAMPUR BERDASARKAN KARAKTERISTI.pdf:application/pdf;Snapshot:/home/fafa/Zotero/storage/7E56T9L5/734.html:text/html},
}

