	langid = {indonesian},
	keywords = {Classification, {ANN}, Deep Learning, {MFCC}, Toronto Emotion Speech Set Data},
	file = {Full Text PDF:/home/fafa/Zotero/storage/7CPR2LGT/Ajrana et al. - 2021 - Implementasi Algoritma Deep Artificial Neural Netw.pdf:application/pdf},
