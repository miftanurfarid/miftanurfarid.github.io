	date = {2019-12},
	keywords = {antenna parameter changes, antenna radiation patterns, Antenna radiation patterns, artificial dielectric, artificial dielectric material effect, Bandwidth, bandwidth 125.0 {MHz}, broadband antennas, conductor diameter, Conductors, conductors (electric), conventional antennas, cylinder conductor, cylindrical conductor, diameter conductor, dielectric materials, Dielectrics, frequency 2.4 {GHz}, gain 7.67 {dB}, inserted conductor, material changes, Microstrip, microstrip antenna, microstrip antennas, Microstrip antennas, modification material, natural dielectrics, resonant frequency, size 1.0 mm to 1.5 mm, size 1.6 mm to 11.2 mm, size 6.4 m, Substrates, {UHF} antennas},
